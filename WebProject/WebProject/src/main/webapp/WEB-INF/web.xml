<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>PersonInfoServlet</servlet-name>
  	<display-name>PersonInfoServlet</display-name>
  	<description></description>
  	<servlet-class>servlets.PersonInfoServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>OperationsServlet</servlet-name>
  	<display-name>OperationsServlet</display-name>
  	<description></description>
  	<servlet-class>servlets.OperationsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>PersonInfoServlet</servlet-name>
  	<url-pattern>/PersonInfoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>OperationsServlet</servlet-name>
  	<url-pattern>/OperationsServlet</url-pattern>
  </servlet-mapping>
</web-app>
